<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        tools:context=".MainMenuFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <Button
                android:text="Persegi Panjang"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/btn_peresegipanjang"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="120dp" android:layout_marginStart="120dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="120dp"
                android:layout_marginRight="120dp" android:layout_marginTop="204dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.0"/>
        <Button
                android:text="Segitiga"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/btn_segitiga" android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/btn_peresegipanjang"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="120dp" android:layout_marginStart="120dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="120dp"
                android:layout_marginRight="120dp" app:layout_constraintHorizontal_bias="0.0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>